{
  "name": "obie-dev",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "lint": "yarn eslint --ignore-path './.eslintignore' './**/*.js'",
    "prebuild": "rimraf public/dist && yarn lint",
    "build": "yarn webpack",
    "build:dev": "yarn build -- --mode development --env.env=dev",
    "build:prod": "NODE_ENV=production yarn build -- --mode production --env.env=prod",
    "watch:dev": "yarn build:dev --watch",
    "watch:prod": "yarn build:prod --watch",
    "start": "nodemon app.js",
    "seed": "sh ./seed.sh",
    "deploy": "forever start app.js >> /var/log/forever/forever.log"
  },
  "author": "Obie <contact@obie.dev>",
  "license": "ISC",
  "dependencies": {
    "@0bie/pattern-lib": "0.0.2",
    "@babel/runtime": "7.3.1",
    "cookie-parser": "1.4.3",
    "express": "4.15.4",
    "express-session": "1.15.6",
    "mongodb": "2.2.33",
    "mongoose": "5.4.6",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "autoprefixer": "9.4.6",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "body-parser": "1.18.3",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.0",
    "errorhandler": "1.5.0",
    "eslint": "5.15.0",
    "method-override": "3.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "morgan": "1.9.1",
    "node-sass": "4.11.0",
    "nodemon": "1.18.9",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-css-variables": "0.11.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "pug": "2.0.3",
